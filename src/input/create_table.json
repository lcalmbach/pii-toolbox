{
    "filename": "test.csv",
    "format": "csv",
    "separator": ";",
    "encoding": "utf-8",
    "rows": 100,
    "columns": {
        "Geschlecht": {
            "data_type": "string",
            "function": "list_string",
            "list": ["M","F"],
            "weights": [1,1],
            "percent_null": 0
        },
        "Name": {
            "data_type": "string",
            "function": "last_name",
            "source": "ogd-bs",
            "percent_null": 0
        },
        "Vorname": {
            "data_type": "string",
            "function": "first_name",
            "source": "ogd-bs",
            "gender_field": "Geschlecht",
            "gender_field_male_identfier": "M",
            "percent_null": 0
        },
        "Kanton": {
            "data_type": "string",
            "function": "list_string",
            "list": ["BS"],
            "percent_null": 0
        },
        "PLZ": {
            "data_type": "int",
            "function": "plz4",
            "canton_field": "Kanton",
            "canton_field_version": "kanton_name_kurz",
            "percent_null": 0
        },
        "Körpergrösse": {
            "data_type": "float",
            "function": "normal_float",
            "digits": 2,
            "min": 1.3,
            "max": 2.0,
            "mean": 1.75,
            "std": 0.2,
            "percent_null": 0    
        },    
        "Herzfrequenz": {
            "data_type": "integer",
            "function": "random_int",
                "min": 90,
                "max": 180,
                "percent_null": 0
        },
        "Temperatur": {
            "data_type": "float",
            "function": "random_float",
            "digits": 2,
            "min": 35,
            "max": 38,
            "percent_null": 0    
        },
        "Messdatum": {
            "data_type": "date",
            "function": "incremental_date",
            "start_date": "2025-01-01",
            "step_period": "day",
            "step": 1   
        },
        "Geburtstag": {
            "data_type": "date",
            "function": "random_date",
            "start_date": "1945-01-01",
            "end_date": "2010-12-31",
            "step": 1   
        },
        "Mutter Geburtstag": {
            "data_type": "date",
            "function": "normal_date",
            "average_date": "1990-01-01",
            "std_days": 730   
        }                        


    }
}
