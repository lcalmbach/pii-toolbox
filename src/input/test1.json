{
    "filename": "test.csv",
    "format": "csv",
    "separator": ";",
    "encoding": "utf-8",
    "rows": 6000,
    "columns": {
        "gender": {
            "data_type": "string",
            "function": "list_string",
            "list": ["M","F"],
            "weights": [1,1],
            "percent_null": 0
        },
        "Name": {
            "data_type": "string",
            "function": "last_name",
            "source": "ogd-bs",
            "percent_null": 0
        },
        "Vorname": {
            "data_type": "string",
            "function": "first_name",
            "source": "ogd-bs",
            "gender_field": "gender",
            "gender_field_male_identfier": "M",
            "percent_null": 0
        },
        "kanton": {
            "data_type": "string",
            "function": "list_string",
            "list": ["BS"],
            "percent_null": 0
        },
        "plz": {
            "data_type": "int",
            "function": "plz4",
            "canton_field": "kanton",
            "canton_field_version": "kanton_name_kurz",
            "percent_null": 0
        },
        "size": {
            "data_type": "float",
            "function": "normal_float",
            "digits": 2,
            "min": 1.3,
            "max": 2.0,
            "mean": 1.75,
            "std": 0.2,
            "percent_null": 0    
        },    
        "Herzfrequenz": {
            "data_type": "integer",
            "function": "random_int",
                "min": 100,
                "max": 200,
                "percent_null": 0
        },
        "Temperatur": {
            "data_type": "float",
            "function": "random_float",
            "digits": 2,
            "min": 35,
            "max": 38,
            "percent_null": 0    
        }    
    }
}
